{
    "extends": "react-app",
    "rules": {
        "semi": 1,
        "quotes": [
            2,
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "comma-dangle": [
            1,
            "always-multiline"
        ],
        "eol-last": [
            1,
            "always"
        ]
    }
}